page{
  background-color: #fafafa;
}
.content{
  width: 100%;
  padding-top: 20rpx;
}
.mycon{
  width: 100%;
  height: 100rpx;
  padding: 0 35rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  font-size: 30rpx;
  border-bottom: 1rpx solid #eee;
}
.mycon-l{
  color: #999;
}
.mycon-r image{
  width: 30rpx;
  height: 30rpx;
}
.submit{
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: flex-end;
}
.submit view{
  width: 560rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  color: #fff;
  background-color: #aaa;
  border-radius: 20rpx;
}
.tcc{
  width: 100%;
  position: fixed;
  top: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, .9)
}
.tcccon{
  width: 600rpx;
  height: 400rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.tcccon-t{
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;

}
.tcccon-b{
  width: 100%;
  height: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.tcccon-b view{
  width: 400rpx;
  height: 100rpx;
  background-color: #f85434;
  color: #fff;
  font-size: 32rpx;
  border-radius: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
