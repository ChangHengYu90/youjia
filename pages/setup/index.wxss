/* pages/setup/index.wxss */
page{
  background-color: #fafafa
}
.setup{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  margin-top: 20rpx;
  padding: 0 35rpx;
  box-sizing: border-box;
  font-size: 30rpx;
}
.setup-r image{
  width: 25rpx;
  height: 25rpx;
}
.bottom{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  color: #fff;
  font-size: 30rpx;
  background-color: #ee0000;
  display: flex;
  justify-content: center;
  align-items: center;
}