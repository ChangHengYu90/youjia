page{
  background-color: #fafafa;
  padding-top: 20rpx;
}
.num{
  width: 100%;
  height: 94rpx;
  display: flex;
  justify-content: space-between;
  padding:0 35rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
  background-color: #fff;
  align-items: center;
}
.num-l{
  width: 460rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.num-l input{
  width: 460rpx;
  height: 100%;
  font-size: 30rpx;
}
.num-r{
  width:155rpx;
  height: 55rpx;
  background-color: #fc6337;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  color: #fff
}
.zhangh{
  width: 100%;
  height: 94rpx;
  padding:0 35rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee; 
  background-color: #fff;
}
.zhangh input{
  width: 100%;
  height: 94rpx;
  font-size: 30rpx;
}
.sfyz{
  width: 100%;
  height: 94rpx;
  padding:0 35rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #000;
}
.sfyz2{
   width: 100%;
  height: 94rpx;
  padding:0 35rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #999;
}
.sfyz2 span{
 
  color: #000;
}
.sfyz2 image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.submit{
  width: 100%;
  height: 140rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
}
.submit view{
  width: 690rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
   background: -webkit-linear-gradient(left, #fe7139 , #f64734); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right,#fe7139 , #f64734); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #fe7139 , #f64734); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #fe7139 , #f64734); /* 标准的语法 */
  color: #fff;
  border-radius: 20rpx;
  font-size: 30rpx;
}