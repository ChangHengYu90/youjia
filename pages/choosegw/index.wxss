.list{
  width: 100%;
  height: 140rpx;
  padding: 0 35rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eee;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list-l{
  height: 100%;
  display: flex;
  flex-flow: row;
}
.list-l-l{
  width: 85rpx;
  height: 100%;
  display: flex;
  align-items: center;
}
.list-l-l image{
  width: 70rpx;
  height:70rpx;
  border-radius: 50%;
}
.list-l-r{
  height: 100%;
  display: flex;
  align-items: center;
}
.list-l-r-t{
  font-size: 30rpx;
  color: #333;
}
.list-l-r-b{
  font-size: 28rpx;
  color: #999;
}
.list-r{
  width: 95rpx;
  height: 55rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #fff;
  background-color: #f25431;
  border-radius: 10rpx;
  
}
.tcc{
    width: 100%;
    position: fixed;
    top: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, .5);
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .tcccon{
    width: 555rpx;
    height: 340rpx;
    background-color: #fff;
    border-radius: 20rpx;
    padding: 60rpx 40rpx;
    box-sizing: border-box;

  }
  .tcccon-t{
    width: 100%;
    height: 170rpx;
    font-size: 32rpx;
    line-height: 1.7;
  }
  .tcccon-t span{
    color: #f2542f
  }
  .tcccon-b{
    width: 100%;
    height: 80rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
  }
  .tcccon-b-l{
    width: 170rpx;
    height: 76rpx;
    border-radius: 15rpx;
    background-color: #fa6137;
    color: #fff;
    font-size: 30rpx;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .tcccon-b-r{
    width: 170rpx;
    height: 76rpx;
    border-radius: 15rpx;
    background-color: #bfbfbf;
    color: #fff;
    font-size: 30rpx;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  .zwsj{
    width: 100%;
    position: fixed;
    top:0;
    bottom: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 30rpx;
    color: #999;
  }