.ygout{
  width: 100%;
  height: 130rpx;
  border-bottom: 1rpx solid #ddd;
  padding: 0 35rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ygout-l{
  height: 100%;
  display: flex;
  align-items: center;
  flex-direction: row;
}
.ygout-r{
  height: 100%;
  display: flex;
  align-items: center;
}
.ygout-r image{
  width: 35rpx;
  height: 35rpx;
}
.ygout-l-l{
  width: 88rpx;
  height: 100%;
  display: flex;
  align-items: center;
}
.ygout-l-l image{
  width: 70rpx;
  height: 70rpx;
}
.ygout-l-r{
  height: 100%;
  display: flex;
  align-items: center
}
.ygout-l-rcon-t{
  font-size: 30rpx;
  padding: 5rpx 0;
}
.ygout-l-rcon-b{
  font-size: 28rpx;
  color: #999;
  padding-top: 5rpx;
}
.bottomkb{
  width: 100%;
  height: 140rpx;
}
.bottom{
  width: 100%;
  height: 140rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
}
.bottomcon{
  width: 690rpx;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: -webkit-linear-gradient(left, #fe7138 , #f54832); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #fe7138 , #f54832); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #fe7138 , #f54832); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #fe7138 , #f54832); /* 标准的语法 */
  color: #fff;
  border-radius: 20rpx;
  font-size: 30rpx;
}